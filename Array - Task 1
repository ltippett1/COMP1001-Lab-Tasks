; A simple example adding two numbers.
.386  ; Specify instruction set
.model flat, stdcall  ; Flat memory model, std. calling convention
.stack 4096 ; Reserve stack space
ExitProcess PROTO, dwExitCode: DWORD  ; Exit process prototype



.data ; data segment

; define your variables here

array DWORD 4,3,5,6 ; this is the array of DWORD

.code ; code segment


main PROC ; main procedure

; write your assembly code here

    mov ecx, [array + TYPE array * 1] ; stores value in second element 
    mov edx, 10h ; store 10h in edx
    move [array + TYPE array * 1], edx ; put 10h in second element of the array

    INVOKE ExitProcess, 0 ; call exit function

main ENDP ; exit main procedure
END main  ; stop assembling 

